<jqa-plugin:jqassistant-plugin name="XML"
                               xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.4">
    <description>Provides scanners for Source Code (currently Python).</description>
    <model>
        <class>org.jqassistant.contrib.plugin.python.api.model.Class</class>
        <class>org.jqassistant.contrib.plugin.python.api.model.Constructor</class>
        <class>org.jqassistant.contrib.plugin.python.api.model.Imports</class>
        <class>org.jqassistant.contrib.plugin.python.api.model.Method</class>
        <class>org.jqassistant.contrib.plugin.python.api.model.Definable</class>
        <class>org.jqassistant.contrib.plugin.python.api.model.Parameter</class>
        <class>org.jqassistant.contrib.plugin.python.api.model.Python</class>
        <class>org.jqassistant.contrib.plugin.python.api.model.PythonFile</class>
        <class>org.jqassistant.contrib.plugin.python.api.model.PythonPackage</class>
        <class>org.jqassistant.contrib.plugin.python.api.model.Signature</class>
        <class>org.jqassistant.contrib.plugin.python.api.model.Value</class>
        <class>org.jqassistant.contrib.plugin.python.api.model.Variable</class>
    </model>
    <scope>
        <class>org.jqassistant.contrib.plugin.python.api.scanner.PythonScope</class>
    </scope>
    <scanner>
        <class>org.jqassistant.contrib.plugin.python.impl.scanner.PackageScannerPlugin</class>
        <class>org.jqassistant.contrib.plugin.python.impl.scanner.PythonFileScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>
